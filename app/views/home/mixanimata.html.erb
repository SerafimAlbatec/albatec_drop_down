<!-- //Script για να εμφανιζω και να κρηβω την πλαγια στηλη στα μηχανιματα -->
<script>

  var a = false;
  $(".mesa_mixanimata").hide();

  //Γινεται πολυ ευκολα με toggle αλλα και ετσι ειναι
  $(document).ready(function(){
  $(".eksw").click(function(){
    if (a === true){
      $(".mesa_mixanimata").show(1000);
      a = false;
    }
    else {
      $(".mesa_mixanimata").show(1000);
      a = true;
    }
  });

  $(".1").click(function(){
    $(".mesa_mixanimata").hide(1000);
    a = false;
  });

  $(".2").click(function(){
    $(".mesa_mixanimata").hide(1000);
    a = false;
  });

  $(".3").click(function(){
    $(".mesa_mixanimata").hide(1000);
    a = false;
  });


});

</script>
